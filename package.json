{
  "name": "pearpass-app-mobile",
  "version": "1.2.1",
  "main": "index.js",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "start": "expo start --dev-client",
    "build": "npm run custom-prebuild && npm run bundle-bare && npm run lingui:extract && npm run lingui:compile",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint:fix": "eslint --fix ./src",
    "lint": "eslint ./src",
    "prepare": "husky",
    "test": "jest",
    "bundle:android": "bare-pack -p android --linked --out bundles/app-android.bundle.js node_modules/pearpass-lib-vault-core/src/worklet/app.js",
    "bundle:ios": "bare-pack -p ios --linked --out bundles/app-ios.bundle.js node_modules/pearpass-lib-vault-core/src/worklet/app.js",
    "bundle:android-extension": "bare-pack -p android --linked --out android/app/src/main/assets/extension.bundle node_modules/pearpass-lib-vault-core/src/worklet/app-ipc.js",
    "bundle:ios-extension": "bare-pack -p ios --linked --out ios/PearPassAutofillExtension/extension.bundle node_modules/pearpass-lib-vault-core/src/worklet/app-ipc.js",
    "bundle-bare": "npm run bundle:android && npm run bundle:ios && npm run bundle:ios-extension && npm run bundle:android-extension",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "custom-prebuild": "node scripts/custom-expo-prebuild.cjs",
    "custom-prebuild:clean": "node scripts/custom-expo-prebuild.cjs --clean",
    "custom-prebuild:ios": "node scripts/custom-expo-prebuild.cjs ios",
    "custom-prebuild-ios:clean": "node scripts/custom-expo-prebuild.cjs ios --clean",
    "custom-prebuild-android:clean": "node scripts/custom-expo-prebuild.cjs android --clean",
    "custom-prebuild:android": "node scripts/custom-expo-prebuild.cjs android",
    "update-submodules": "bash scripts/update-submodules.sh",
    "eas-build-pre-install": "node -v && npm -v && npm install --package-lock-only --ignore-scripts --no-audit --no-fund",
    "eas-build-post-install": "npm run bundle-bare && npm run lingui:extract && npm run lingui:compile"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
